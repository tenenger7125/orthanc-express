{
  "name": "express-mysql-server",
  "version": "0.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "src/app.ts",
  "scripts": {
    "preinstall": "[ -f dcmjs.d.ts ] ||npx dts-gen -m dcmjs",
    "prestart": "pnpm db:migrate && pnpm db:model:auto-generate",
    "start": "ts-node-dev --ignore-watch . --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "dev": "pnpm db:seed:init && pnpm start",
    "db:migrate": "sequelize db:migrate",
    "db:seed:init": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "db:model:auto-generate": "ts-node-dev -r tsconfig-paths/register src/database/orm.ts"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "dcmjs": "^0.42.0",
    "dcmjs-dimse": "^0.2.7",
    "dcmjs-imaging": "^0.2.1",
    "dicom-dimse-native": "^2.4.6",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "gitmoji-cli": "^9.7.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.1",
    "sequelize": "^6.37.7",
    "sequelize-auto": "^0.8.8",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.4",
    "dotenv": "^17.0.0",
    "dts-gen": "^0.10.4",
    "eslint": "^9.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.2.0",
    "joi": "^17.13.3",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  }
}
